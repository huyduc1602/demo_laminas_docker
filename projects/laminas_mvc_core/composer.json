{
    "name": "mezzio/mezzio-skeleton",
    "description": "Zend expressive skeleton. Begin developing PSR-7 middleware applications in seconds!",
    "require": {
        "php": "^7.4 | ^8.0",
        "laminas/laminas-stdlib": "^3.7",
        "laminas/laminas-servicemanager": "^3.0",
        "laminas/laminas-mvc": "^3.1.1",
		"laminas/laminas-cache": "^3.1.2",
        "laminas/laminas-mvc-form": "^2.0",
        "laminas/laminas-mvc-plugins": "^1.2",
		"laminas/laminas-mvc-middleware": "^2.2",
		"laminas/laminas-di": "^3.2",
        "laminas/laminas-math": "^3.0",
        "laminas/laminas-crypt": "^3.1",
        "laminas/laminas-captcha": "^2.6",
        "laminas/laminas-session": "^2.10",
        "laminas/laminas-authentication": "^2.9",
        "laminas/laminas-mail": "^2.16",
        "laminas/laminas-config": "^3.7",
        "laminas/laminas-i18n": "^2.7",
		"laminas/laminas-mvc-i18n": "^1.2.0",
		"laminas/laminas-test": "^4.0",
        "laminas/laminas-text": "^2.7",
		"laminas/laminas-serializer": "^2.9",
        "laminas/laminas-file": "^2.8",
		"laminas/laminas-dom": "^2.8.0",
        "html2text/html2text": "^4.3",
        "laminas/laminas-log": "^2.13.1",
		"laminas/laminas-paginator": "^2.10",
		"laminas/laminas-view": "^2.12",
		"doctrine/orm": "^2.7",
        "beberlei/doctrineextensions": "^1.3",
        "doctrine/doctrine-module": "^6.0",
        "doctrine/doctrine-orm-module": "^6.0",
        "defuse/php-encryption": "^2.0",
        "payjp/payjp-php": "~1.0",
        "kunalvarma05/dropbox-php-sdk": "^0.3",
        "gumlet/php-image-resize": "^2.0",
        "symfony/filesystem": "^4.2",
        "enqueue/rdkafka": "^0.10",
        "matomo/device-detector": "^6.0",
        "phpseclib/phpseclib": "^3.0",
        "monolog/monolog": "^2.3",
        "laminas/laminas-cache-storage-adapter-redis": "^2.0",
		"laminas/laminas-cache-storage-adapter-filesystem": "^2",
        "sngrl/php-firebase-cloud-messaging": "dev-master",
        "web-token/jwt-framework": "^3.0",
        "square/square": "^27",
		"league/oauth2-facebook": "^2.2",
        "zbateson/mail-mime-parser": "^2.4",
        "openai-php/client": "^0.8.1",
        "ggergo/sqlindexhintbundle": "^1.1",
        "elasticsearch/elasticsearch": "^8.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "filp/whoops": "^2.0",
		"symfony/var-dumper": "^6.0"
    },
    "license": "BSD-3-Clause",
    "autoload-dev": {
        "psr-4": {}
    },
    "autoload": {
        "psr-4": {
            "Zf\\Ext\\": "zf-ext/"
        },
		"files": [
			"zf-ext/Utilities/GuzzleHttpStreamFor.php"
        ]
    },
    "type": "project",
    "scripts": {
        "check": [
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "serve": "php -S 0.0.0.0:8080 -t public public/api/index.php",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "upload-coverage": "coveralls -v"
    },
    "homepage": "https://github.com/mezzio/mezzio-skeleton",
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": false,
            "php-http/discovery": true
        }
    }
}
